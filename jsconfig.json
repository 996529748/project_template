{
  "compilerOptions": {
    "experimentalDecorators": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "common/*": ["src/common/*"],
      "api/*": ["src/api/*"],
      "components/*": ["src/components/*"],
      "config/*": ["src/config/*"],
      "base/*": ["src/base/*"],
      "plugin/*": ["src/plugin/*"],
      "utils/*": ["src/utils/*"],
      "class/*": ["src/class/*"],
      "pages/*": ["src/pages/*"],
      "views/*": ["src/views/*"],
      "mixins/*": ["src/mixins/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
